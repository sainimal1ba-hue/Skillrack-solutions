int main(){
   int a,b,c;
   scanf("%d %d %d",&a,&b,&c);
   if(a==0||b==0||c==0){
      if(a==0 && b==0 && c==0){
         printf("3");
      }else if((a==0 && b>0 && c>0)||(b==0 && a>0 && c>0)||(c==0 && b>0 && a>0)){
         printf("1");
      }else{
         printf("2");
      }
   }else{
      printf("0");
   }
}
