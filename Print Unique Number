#include <stdio.h>
#include <stdlib.h>
int main(){
   int a,b,c;
   scanf("%d %d %d",&a,&b,&c);
   if(a==c && a!=b && c!=b){
       printf("%d",b);
   }else if(a==b && a!=c && b!=c){
       printf("%d",c);
   }else if(b==c && a!=b && a!=c){
       printf("%d",a);
   }else{
       printf("-1");
   }
}
